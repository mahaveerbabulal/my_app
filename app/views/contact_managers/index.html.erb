<p id="notice"><%= notice %></p>

<h1>Listing Contact Managers</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Mobile</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contact_managers.each do |contact_manager| %>
      <tr>
        <td><%= contact_manager.first_name %></td>
        <td><%= contact_manager.last_name %></td>
        <td><%= contact_manager.mobile %></td>
        <td><%= contact_manager.email %></td>
        <td><%= link_to 'Show', contact_manager %></td>
        <td><%= link_to 'Edit', edit_contact_manager_path(contact_manager) %></td>
        <td><%= link_to 'Destroy', contact_manager, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contact manager', new_contact_manager_path %>
